{
	"name": "flutter",
	"image": "georgespanos/flutter:latest",
	"mounts": [
		"source=/dev/bus/usb,target=/dev/bus/usb,type=bind"
	],
	"extensions": [
		"dart-code.flutter"
	],
	"runArgs": [
		"--privileged"
	],
	"workspaceMount": "source=${localWorkspaceFolder}/workspace,target=/src,type=bind,consistency=delegated",
	"workspaceFolder": "/src"
}